    </PropertyGroup>

    <ItemGroup>
        <Compile Include="Library.fs"/>
        <Compile Include="Numbers.fs"/>
        <Compile Include="Vector.fs"/>
        <Compile Include="Matrix.fs"/>
    </ItemGroup>

</Project>
