        <PublishSingleFile>true</PublishSingleFile>
        <UseAppHost>true</UseAppHost>
        <AssemblyName>Ganith</AssemblyName>
        <RootNamespace>Ganith</RootNamespace>
        <LangVersion>8.0</LangVersion>
